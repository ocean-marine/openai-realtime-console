{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/token.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/token",
      "destination": "/api/token"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
