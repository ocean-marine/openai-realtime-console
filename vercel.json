{
  "version": 2,
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/client",
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/token",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}