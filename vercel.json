{
  "version": 2,
  "buildCommand": "npm run build:client && mv dist/client client",
  "outputDirectory": "client",
  "functions": {
    "api/server.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/token",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
